<?xml version="1.0" encoding="utf-8"?>
<Application xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="fullbar.App">
	<Application.Resources>

		 <ResourceDictionary>
		<Style x:Key="EstiloMestre" TargetType="Button">
				<Setter Property="BackgroundColor" Value="Blue"/>
		</Style>
		<Style x:Key="EstiloTeste" TargetType="Button" BaseResourceKey="EstiloMestre"
				>
				<Setter Property="FontSize" Value="16" />
				<Setter Property="BorderRadius" Value="22" />
				<Setter Property="HeightRequest" Value="44"/>
				<Setter Property="WidthRequest" Value="150"/>

				<Setter Property="TextColor" Value="Gray"/>
		</Style>
		</ResourceDictionary>
	</Application.Resources>
</Application>
